{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "./types/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@slices/*": [
        "redux/slices/*"
      ],
      "@redux/*": [
        "redux/*"
      ],
      "@layout/*": [
        "layout/*"
      ],
      "@src/*": [
        "src/*"
      ],
    },
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "types"
  ]
}
